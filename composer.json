{
    "name": "tonybogdanov/mockable-annotations",
    "description": "Mockable Doctrine annotations",
    "type": "library",
    "require": {
        "doctrine/annotations": "^1.10",
        "php-64bit": "^7.3",
        "doctrine/cache": "^1.10"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.1",
        "jaschilz/php-coverage-badger": "^2.0",
        "ext-pcov": "^1.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Tony Bogdanov",
            "email": "support@tonybogdanov.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "TonyBogdanov\\MockableAnnotations\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TonyBogdanov\\MockableAnnotations\\Test\\": "tests/"
        }
    },
    "config": {
        "platform": {
            "php": "7.3"
        }
    }
}
